{
  "name": "redis",
  "version": "1.0.0",
  "description": "redis server",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "start": "node dist/index.js",
    "start:watch": "nodemon dist/index.js",
    "dev": "concurrently \"npm run build:watch\" \"npm run start:watch\""
  },
  "dependencies": {
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@types/node": "^25.2.0",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.0"
  }
}
